{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["es2020", "dom"],
    "allowJs": false,
    "noEmit": true,
    "module": "ES2020",
    "target": "es2020",
    "moduleResolution": "node",
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
}