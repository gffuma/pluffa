{
  "name": "snext",
  "type": "module",
  "description": "Build static sites with React",
  "version": "0.5.2",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./render": {
      "require": "./dist/render.cjs",
      "import": "./dist/render.js"
    },
    "./statik": {
      "browser": {
        "require": "./dist/lib/statik.browser.cjs",
        "import": "./dist/lib/statik.browser.js"
      },
      "node": {
        "require": "./dist/lib/statik.node.cjs",
        "import": "./dist/lib/statik.node.js"
      },
      "default": "./dist/lib/statik.browser.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "statik": [
        "./dist/lib/statik.d.ts"
      ],
      "render": [
        "./dist/render.d.ts"
      ]
    }
  },
  "types": "./dist/index.d.ts",
  "author": "Giovanni Fumagalli",
  "license": "MIT",
  "bin": {
    "snext": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "repository": "gffuma/snext",
  "keywords": [
    "react",
    "static site"
  ],
  "dependencies": {
    "webpack": "^5.69.0",
    "chalk": "^5.0.0",
    "commander": "^9.0.0",
    "express-http-proxy": "^1.6.3",
    "itty-router": "^2.4.10",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "node-html-parser": "^5.2.0",
    "p-queue": "^7.2.0",
    "rimraf": "^3.0.2",
    "stream-buffers": "^3.0.2",
    "@snext/build-tools": "^0.1.0"
  },
  "devDependencies": {
    "@snext/cloudflare-worker": "^0.1.0",
    "@types/express": "^4.17.13",
    "@types/express-http-proxy": "^1.6.3",
    "@types/mkdirp": "^1.0.2",
    "@types/ncp": "^2.0.5",
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/rimraf": "^3.0.2",
    "@types/stream-buffers": "^3.0.4"
  },
  "peerDependencies": {
    "react": "^18.0.0-rc.1",
    "react-dom": "^18.0.0-rc.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
