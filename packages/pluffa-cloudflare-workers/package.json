    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "webpack": "^5.73.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@types/express": "^4.17.13",
