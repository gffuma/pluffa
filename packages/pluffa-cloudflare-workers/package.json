{
  "name": "@pluffa/cloudflare-workers",
  "type": "module",
  "description": "Pluffa on cloudflare workers",
  "version": "0.7.1",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
