{
  "name": "@snext/cloudflare-worker",
  "type": "module",
  "description": "SNext on cloudflare worker",
  "version": "0.6.0",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./render": {
      "require": "./dist/render.cjs",
      "import": "./dist/render.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "render": [
        "./dist/render.d.ts"
      ]
    }
  },
  "types": "./dist/index.d.ts",
  "author": "Giovanni Fumagalli",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "repository": "gffuma/snext",
  "keywords": [
    "react",
    "static site"
  ],
  "engines": {
    "node": ">=16.7"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@snext/build-tools": "^0.2.0",
    "chalk": "^5.0.0",
    "http-proxy-middleware": "^2.0.3",
    "miniflare": "^2.3.0",
    "ncp": "^2.0.0",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "webpack": "^5.69.0"
  },
  "peerDependencies": {
    "react": "^18.0.0-rc.2",
    "react-dom": "^18.0.0-rc.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/ncp": "^2.0.5"
  }
}
